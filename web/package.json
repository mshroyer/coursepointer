{
  "name": "web",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "pnpm run build:geographiclib && pnpm run build:coursepointer && tsc && vite build",
    "build:geographiclib-direct": "em++ -O3 -std=c++11 '-sEXPORTED_RUNTIME_METHODS=[\"wasmMemory\",\"HEAPU8\",\"UTF8ToString\"]' '-sEXPORTED_FUNCTIONS=[\"_geodesic_inverse_with_azimuth\",\"_geodesic_direct\",\"_gnomonic_forward\",\"_gnomonic_reverse\",\"_geocentric_forward\",\"_get_geographiclib_version\",\"_get_compiler_version\"]' -I../include -I../geographiclib/include ../geographiclib/src/DST.cpp ../geographiclib/src/EllipticFunction.cpp ../geographiclib/src/Geocentric.cpp ../geographiclib/src/Geodesic.cpp ../geographiclib/src/GeodesicExact.cpp ../geographiclib/src/GeodesicLine.cpp ../geographiclib/src/GeodesicLineExact.cpp ../geographiclib/src/Gnomonic.cpp ../geographiclib/src/Math.cpp ../src/shim.cpp --no-entry -o src/wasm/geographiclib.mjs",
    "build:geographiclib": "make -C src/wasm all",
    "build:coursepointer": "wasm-pack build coursepointer-wasm",
    "clean": "rm src/wasm/geographiclib.*",
    "preview": "vite preview"
  },
  "devDependencies": {
    "typescript": "~5.8.3",
    "vite": "^7.0.4"
  },
  "dependencies": {
    "coursepointer-wasm": "link:coursepointer-wasm/pkg"
  }
}
